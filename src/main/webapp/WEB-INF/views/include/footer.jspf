<div class="loginmodalbox modal fade" id="ritekhelamodalrg"
	tabindex="-1">
	<div class="modal-dialog modal-dialog-centered" role="document">
		<div class="modal-content">
			<div class="modal-body ritekhela-bgcolor-two">
				<h5 class="modal-title">회원가입</h5>
				<a href="#" class="close ritekhela-bgcolor-two" data-dismiss="modal"
					aria-label="Close"> <i class="fa fa-times"></i>
				</a>
				<form action="/user/register" method="post" id="register"
					class="loginmodalbox-search">
					<input type="text" name="userid" id="userid" placeholder="아이디">
					<input type="password" name="userpw" id="userpw" placeholder="비밀번호">
					<input type="password" name="confirmpw" id="confirmpw"
						placeholder="비밀번호 확인"> <input type="text" name="username"
						id="username" placeholder="이름"> <input type="text"
						name="usernname" id="usernname" placeholder="닉네임"> <input
						type="email" name="useremail" id="useremail" placeholder="이메일">
					<select name="userage" id="userage">
						<option>나이</option>
						<%
							for (int i = 1; i < 100; i++) {
						%>
						<option value="<%=i%>"><%=i%>세
						</option>
						<%
							}
						%>
					</select>

					<div id="radio">
						성별 : <input type="radio" name="usergender" value="M" checked>
						남자 <input type="radio" name="usergender" value="F"> 여자<br>
					</div>
					<input type="text" name="usertel" id="usertel"
						placeholder="전화번호('-'를 제외하고 입력하여 주세요)"> <input type="text"
						name="userloc" id="userloc" placeholder="거주지(도 , 시 까지만 입력하여 주세요)">
					<div id="radio">
						포지션 : <input type="radio" name="userposition" value="FW">
						FW <input type="radio" name="userposition" value="DF"> DF
						<input type="radio" name="userposition" value="MF"> MF <input
							type="radio" name="userposition" value="GK"> GK <input
							type="radio" name="userposition" value="ALL" checked> ALL
					</div>
					<textarea name="userintro" id="userintro"
						placeholder="자기소개를 입력해 주세요"></textarea>


					<input type="submit" value="회원가입" class="ritekhela-bgcolor">
					<a href="#" class="loginmodalbox-forget" data-toggle="modal"
						data-target="#ritekhelamodalsearch"><i class="fas fa-key"></i>
						비밀번호 찾기</a>
				</form>
				<div class="ritekhela-loginbox-social">
					<ul>
						<li><a href="#"><i class="fab fa-facebook-f"></i>
								Facebook</a></li>
						<li class="twitter"><a href="#"><i class="fab fa-twitter"></i>
								Twitter</a></li>
						<li class="linkedin"><a href="#"><i
								class="fab fa-linkedin-in"></i> Linkedin</a></li>
						<li class="dribbble"><a href="#"><i
								class="fab fa-dribbble"></i> Dribbble</a></li>
					</ul>
				</div>
			</div>

		</div>

	</div>
	<script>
		$("#userid").on("blur", function(event) {

			$.ajax({
				url : '/user/idcheck',
				data : {
					'userid' : $('#userid').val()
				},
				dataType : 'json',
				type : 'POST',
				success : function(data) {
					if (data == 1) {
						alert("이미 존재하는 아이디 입니다");
							$('#userid').val('');
							$('#userid').focus();
						
					} else if (data == 0) {
						if ($('#userid').val() != "") {
							alert("사용 가능한 아이디 입니다");
							
						}
					}
				}
			});
		});
		$("#useremail").on("blur", function(event){
			  var email = $("#useremail").val();
			  var regExp = /^([0-9a-zA-Z_\.-]+)@([0-9a-zA-Z_-]+)(\.[0-9a-zA-Z_-]+){1,2}$/;

			  if ((email.match(regExp) == null) && (email != "")) {
				  alert('이메일 형식이 올바르지 않습니다');
				  $('#useremail').val('');
				  $('#useremail').focus();
				  return false;
			  }
		});
		$("#usernname").on("blur", function(event) {

			$.ajax({
				url : '/user/nnamecheck',
				data : {
					'usernname' : $('#usernname').val()
				},
				dataType : 'json',
				type : 'POST',
				success : function(data) {
					if (data == 1) {
						alert("이미 존재하는 닉네임 입니다");
							$('#usernname').val('');
							$('#usernname').focus();
						
					} else if (data == 0) {
						if ($('#usernname').val() != "") {
							alert("사용 가능한 닉네임 입니다");
							
						}
					}
				}
			});
		});
		$("#register").submit(
				function(e) {
					if (($.trim($("#userid").val()) == "")) {
						alert("아이디를 입력하여 주세요");
						return false;
					}
					if (($.trim($("#userpw").val()) == "")) {
						alert("비밀번호를 입력하여 주세요");
						return false;
					}
					if (($.trim($("#confirmpw").val()) == "")) {
						alert("비밀번호확인을 입력하여 주세요");
						return false;
					}
					if (($.trim($("#userpw").val())) != ($.trim($("#confirmpw")
							.val()))) {
						alert("비밀번호가 일치하지 않습니다");
						return false;
					}
					if (($.trim($("#username").val()) == "")) {
						alert("이름을 입력하여 주세요");
						return false;
					}
					if (($.trim($("#usernname").val()) == "")) {
						alert("닉네임을 입력하여 주세요");
						return false;
					}
					if (($.trim($("#useremail").val()) == "")) {
						alert("이메일을 입력하여 주세요");
						return false;
					}
					if (($.trim($("#userage").val()) == "나이")) {
						alert("나이를 입력하여 주세요");
						return false;
					}
					if (($.trim($("#usertel").val()) == "")) {
						alert("전화번호를 입력하여 주세요");
						return false;
					}
					if (($.trim($("#userloc").val()) == "")) {
						alert("거주지를 입력하여 주세요");
						return false;
					}
					if (($.trim($("#userintro").val()) == "")) {
						alert("자기소개를 입력하여 주세요");
						return false;
					}
				});
	</script>
</div>

