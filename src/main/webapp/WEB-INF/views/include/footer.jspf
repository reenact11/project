<div class="loginmodalbox modal fade" id="ritekhelamodalcenter"
	tabindex="-1">
	<div class="modal-dialog modal-dialog-centered" role="document">
		<div class="modal-content">
			<div class="modal-body ritekhela-bgcolor-two">
				<h5 class="modal-title">로그인</h5>
				<a href="#" class="close ritekhela-bgcolor-two" data-dismiss="modal"
					aria-label="Close"> <i class="fa fa-times"></i>
				</a>
				<form action="/user/loginPost" method="post" id="login"
					class="loginmodalbox-search">
					<input type="text" name="userid" id="id" placeholder="아이디 입력">
					<input type="password" name="userpw" id="pw" placeholder="비밀번호 입력">
					<input type="submit" value="로그인" class="ritekhela-bgcolor">
					<div style=float:left;><label class="loginmodalbox-forget"><input type="checkbox" name="useCookie"> 자동 로그인</label></div>
					<a href="#" class="loginmodalbox-forget" data-toggle="modal"
						data-target="#ritekhelamodalsearch"><i class="fas fa-key"></i>
						비밀번호 찾기</a> <br></br> <a href="#" class="loginmodalbox-forget"
						data-toggle="modal" data-target="#ritekhelamodalrg"><i
						class="fa fa-sign-in-alt"></i> 회원가입</a>
				</form>
				<div class="ritekhela-loginbox-social">
					<ul>
						<li><a href="#"><i class="fab fa-facebook-f"></i>
								Facebook</a></li>
						<li class="twitter"><a href="#"><i class="fab fa-twitter"></i>
								Twitter</a></li>
						<li class="linkedin"><a href="#"><i
								class="fab fa-linkedin-in"></i> Linkedin</a></li>
						<li class="dribbble"><a href="#"><i
								class="fab fa-dribbble"></i> Dribbble</a></li>
					</ul>

				</div>
				<!-- <a href="#" id="login">test...</a> -->
			</div>
		</div>
	</div>


	<script>
		$("#login").submit(function(e) {
			if (($.trim($("#id").val()) == "")) {
				alert("아이디를 입력하여 주세요");
				return false;
			}
			if (($.trim($("#pw").val()) == "")) {
				alert("비밀번호를 입력하여 주세요");
				return false;
			}
		});
	</script>
</div>

